<%= form_for :user, url: {action: "create"} do |f| %>
  <%= f.label :first_name %>:
  <%= f.text_field :first_name %><br />

  <%= f.label :last_name %>:
  <%= f.text_field :last_name %><br />

  <%= f.label :phone %>:
  <%= f.text_field :phone %><br />

  <%= f.label :email %>:
  <%= f.text_field :email %><br />

  <%= f.label :cover_letter %>:
  <%= f.text_area :cover_letter %><br />


  <%= f.submit "Apply" %>
<% end %>